{
    "builds": [
        {
          "src": "app", 
          "use": "@vercel/node" 
        },
        {
          "src": "api", 
          "use": "@now/python" 
        }
      ],
    "routes": [
        {
            "src": "/api/((?!arima).*)",
            "dest": "api/arima" 
        },
        {
            "src": "/app/(.*)",
            "dest": "app/$1"
        }
    ]
}