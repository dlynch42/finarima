{
    "builds": [
        {
          "src": "app", 
          "use": "@vercel/node" 
        },
        {
          "src": "api", 
          "use": "@now/python" 
        }
      ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "api/arima.py" 
        },
        {
            "src": "/(.*)",
            "dest": "app/$1"
        }
    ]
}